function loadXwsData()
  --print("Loading xws")
  WebRequest.get('https://raw.githubusercontent.com/guidokessels/xwing-data2/master/data/manifest.json', function(request)
      if request.is_error then
          print("Failed to load manifest.json which is required to get updated point costs.")
      elseif request.is_done then
          print("Received xws data manifest")
          XWS_DATA.manifest = table.deepcopy(json.parse(request.text))
          loadXwsDataShips()
      end
  end)
  WebRequest.get('https://raw.githubusercontent.com/guidokessels/xwing-data2/master/data/factions/factions.json', function(request)
      if request.is_error then
          print("Failed to load factions.json which is required to get updated point costs.")
      elseif request.is_done then
          XWS_DATA.factions = {}
          print("Received xws factions")
          for _, faction in pairs(json.parse(request.text)) do
            XWS_DATA.factions[faction.xws] = faction
          end
          loadXwsDataShips()
      end
  end)
end

function loadXwsDataShips()
  if XWS_DATA.manifest and XWS_DATA.factions then
    for _, faction in pairs(XWS_DATA.manifest.pilots) do
      --print("Faction: " .. XWS_DATA.factions[faction.faction].name)
      --XWS_DATA.factions[faction.faction].ships = {}
      for _,ship in pairs(faction.ships) do
        --print(" - " .. ship)
        WebRequest.get('https://raw.githubusercontent.com/guidokessels/xwing-data2/master/' .. ship, function(request)
          if request.is_error then
            print('Unable to retreive data from "https://raw.githubusercontent.com/guidokessels/xwing-data2/master/' .. ship .. '"')
          else
            shipdata = JSON.decode(request.text)
            --shipdata = json.parse(request.text)
            --XWS_DATA.factions[faction.faction].ships[shipdata.xws] = shipdata
            for _, pilot in pairs(shipdata.pilots) do
              XWS_DATA.pilots[pilot.xws] =  {cost = pilot.cost, loadout = pilot.loadout, slots = pilot.slots}--pilot
            end -- for shipdata.pilots
          end -- if request.is_error
        end) -- webrequest
      end -- for faction.ships
    end -- for manifest.pilots

    for _, upgrade in pairs(XWS_DATA.manifest.upgrades) do
      --print("Upgrade: " .. upgrade)
      WebRequest.get('https://raw.githubusercontent.com/guidokessels/xwing-data2/master/' .. upgrade, function(request)
        if request.is_error then
          print('Unable to retreive data from "https://raw.githubusercontent.com/guidokessels/xwing-data2/master/' .. upgrade .. '"')
        else
          local upgradedata = JSON.decode(request.text)
          for _, upgrade in pairs(upgradedata) do
            XWS_DATA.upgrades[upgrade.xws] = {cost = upgrade.cost} --upgrade
          end -- for upgradedata.upgrade
        end -- if request.is_error
      end) -- webrequest
    end -- for manifest.upgrades
  end -- if manifest..
end --function


function findByXws(table, xws_id)
  for i, record in pairs(table) do
    if record.XWS and record.XWS == xws_id then
      return i
    end
  end
  return nil
end

function listToFormat(version, vendor)
  if string.find(version, '2.0.') then
    return "2.0"
  else
    return "2.5"
